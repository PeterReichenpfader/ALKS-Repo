        <packagedElement xmi:type="uml:Class" xmi:id="_j-RgwGIbEeuVR7rbTVzdsQ" name="6.4.2.2 System status display when activated "/>
        <packagedElement xmi:type="uml:Class" xmi:id="_o5qcsGIbEeuVR7rbTVzdsQ" name="6.4.2.3 System status display when deactivated"/>
